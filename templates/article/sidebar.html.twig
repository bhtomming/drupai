<div class="card">
    <h5 class="card-title text-center mt-5">相关链接</h5>
    <div class="card-body">
        <ul class="list-group list-group-flush">
            {% for article in articles %}
                <li class="list-group-item"><a href="{{ path('article_show',{'slug': article.slug }) }}">{{ article.title }}</a></li>
            {% endfor %}
        </ul>
    </div>
</div>
