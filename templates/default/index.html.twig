{% extends 'base.html.twig' %}

{% block title %}北海助派网络_网站制作_小程序开发_营销推广_{{ parent() }}首页{% endblock %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('build/css/swiper.min.css') }}">
    {{ parent() }}
{% endblock %}

{% block body %}
    {% block slider %}
    {#{{ include('/default/swiper.html.twig') }}#}
        {{ region('slider') }}
        {% endblock %}
    {% block height_light %}
    {#{{ include('/default/business.html.twig') }}#}
        {{ region('height_light') }}
        {% endblock %}
    {% block seconde_row %}
    {#{{ include('/default/template.html.twig') }}#}
        {{ region('seconde_row') }}
        {% endblock %}
    {% block third_row %}
    {#{{ include('/default/integlar.html.twig') }}#}
        {{ region('third_row') }}
        {% endblock %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {#<script src="{{ asset('build/js/jquery-1.9.1.min.js') }}"></script>#}
    <script src="{{ asset('build/js/swiper.min.js') }}"></script>
    <script>
        $(document).ready(function () {
            var frontSwiper = new Swiper('#swiper-container-hd',{
                loop: true,
                pagination: {
                    el: '.swiper-pagination'
                },
                preventLinksPropagation: true,
                autoplay:{
                    delay:4000,
                    disableOnInteraction: false
                },
                touchMoveStopPropagation: false
            });
        });
    </script>
{% endblock %}

