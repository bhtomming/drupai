{% extends 'base.html.twig' %}

{% block title %}{{ city_page.mainWord }}_{{ parent() }}{% endblock %}
{% block keywords %}{{ city_page.keywords }}{% endblock %}
{% block description %}{{ city_page.description }}{% endblock %}

{% if city_page.parentPage %}
    {% set parent = city_page.parentPage %}
    {% if parent.parentPage  %}
        {% set faparent = parent.parentPage %}
    {% endif %}
{% endif %}
{% if city_page.subLinks %}
    {% set sub_links = city_page.subLinks %}
{% endif %}

{% set reglink = 'http://shop.drupai.com/admin/signup.php' %}

{% block body %}
    <div class="container py-5" id="page-city-article">

        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/">首页</a></li>
                {% if  faparent is defined %}
                    <li class="breadcrumb-item"><a href="{{ faparent.slug }}">{{ faparent.mainWord }}小程序制作</a></li>
                {% endif %}
                {% if parent is defined %}
                <li class="breadcrumb-item"><a href="{{ parent.slug }}">{{ parent.mainWord }}小程序制作</a></li>
                {% endif %}
                <li class="breadcrumb-item active" aria-current="page">{{ city_page.mainWord }}小程序制作</li>
            </ol>
        </nav>

        <div class="row mt-3">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">相关区域</div>
                    <div class="card-body">
                        <div class="row">
                            {% for same_area in sub_links %}
                                <div class="col-md-2 col-4">
                                    <a  href="{{ same_area.slug }}">{{ same_area.mainWord }}</a>
                                </div>
                            {% else %}
                                {% for same_city in parent.subLinks %}
                                    <div class="col-md-2 col-4"><a  href="{{ same_city.slug }}">{{ same_city.mainWord }}</a></div>
                                {% else %}
                                {% endfor %}
                            {% endfor %}
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <div class="row mt-3">
            <div class="col-md-9">

                <div class="card mb-3">
